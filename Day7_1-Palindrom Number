import java.util.*;
public class Palindrom{
    public static void main(String args[]){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int result=0,m=n;
        while(n>0){
            int r=n%10;
            result=result*10+r;
            n=n/10;
        }
        if(result==m)
        System.out.println("Palindrom Number");
        else
        System.out.println("Not a Palindrom Number");
    }
}
